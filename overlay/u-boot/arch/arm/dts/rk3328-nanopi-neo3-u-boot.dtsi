#include "rk3328-u-boot.dtsi"
#include "rk3328-sdram-ddr4-666.dtsi"
/ {
  aliases {
    /delete-property/ mmc0;
  };

	chosen {
		u-boot,spl-boot-order = "same-as-spl", &sdmmc;
	};
};

&emmc {
  status = "disabled";
};

&gpio0 {
	u-boot,dm-spl;
};

&pinctrl {
	u-boot,dm-spl;
};

&sdmmc0m1_gpio {
	u-boot,dm-spl;
};

&pcfg_pull_up_4ma {
	u-boot,dm-spl;
};

/* Need this and all the pinctrl/gpio stuff above to set pinmux */
&vcc_sd {
	u-boot,dm-spl;
};
